{
  "name": "phoenix-lib/marketplace-php-partner",
  "description": "PHP library of Partners Marketplace API.",
  "keywords":["yandex","sdk", "market", "beru"],
  "type":"library",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6",
    "ext-curl": "*",
    "ext-json": "*",
    "phoenix-lib/yandex-php-common": "1.*.*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "php-coveralls/php-coveralls": "^2.0",
    "phpunit/phpunit": "5.7.6"
  },
  "autoload": {
    "psr-0": {
      "Yandex": "src/",
      "Yandex\\Tests": "tests/"
    }
  },
  "scripts": {
    "test" : "php vendor/bin/phpunit --configuration tests/phpunit.xml.dist --coverage-clover=.reports/clover.xml"
  }
}
